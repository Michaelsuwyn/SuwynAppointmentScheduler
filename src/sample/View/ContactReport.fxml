<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="544.0" prefWidth="864.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.ContactReportController">
   <children>
      <TableView fx:id="contactTable" layoutX="29.0" layoutY="73.0" prefHeight="418.0" prefWidth="701.0">
        <columns>
          <TableColumn fx:id="apptID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="apptTitle" prefWidth="75.0" text="Title" />
            <TableColumn fx:id="apptType" prefWidth="90.0" text="Type" />
            <TableColumn fx:id="apptDesc" prefWidth="99.0" text="Description" />
            <TableColumn fx:id="apptStart" prefWidth="123.0" text="Start" />
            <TableColumn fx:id="apptEnd" prefWidth="130.0" text="End" />
            <TableColumn fx:id="apptCustomerID" prefWidth="107.0" text="Customer ID" />
        </columns>
      </TableView>
      <RadioButton fx:id="anikaRadio" layoutX="119.0" layoutY="39.0" mnemonicParsing="false" onAction="#anikaChoice" text="Anika Costa" />
      <RadioButton fx:id="danielRadio" layoutX="257.0" layoutY="39.0" mnemonicParsing="false" onAction="#danielChoice" text="Daniel Garcia" />
      <RadioButton fx:id="leeRadio" layoutX="405.0" layoutY="39.0" mnemonicParsing="false" onAction="#leeChoice" text="Li Lee" />
   </children>
</AnchorPane>
